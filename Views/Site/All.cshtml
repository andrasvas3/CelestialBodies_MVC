<h1 class="title has-text-centered">All planets and moons</h1>
<br />
<table class="table is-bordered is-striped is-hoverable is-fullwidth">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Planet</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Moon moon in (IEnumerable<Moon>)ViewData["Moons"]!)
        {
            <tr>
            <td>@moon.Id</td>
            <td>@moon.Name</td>
            <td>@moon.Planet!.Name</td>
            </tr>
        }
    </tbody>
</table>
<br />
